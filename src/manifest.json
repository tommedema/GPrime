{
    "name": "GPrime",
    "version": "0.0.1",
    "description": "Efficient googling. This extension is not endorsed by or affiliated with Google Inc.",
    "dev_comments": "Matches and js fields are automatically generated by build process. Bootjs entries are included before routines, then routines (.js files in routines folder), then postbootjs entries.",
    "content_scripts": [
        {
            "domains": [
                "com",
                "nl"
            ],
            "bootjs": [
                "lib/eventemitter2.js",
                "lib/eventemitter2.chains.js",
                "lib/mediator.js"
            ],
            "postbootjs": [
                "routines/postboot.js"
            ],
            "css": [
                "prime.css"
            ],
            "run_at": "document_start",
            "js": [
                "lib/eventemitter2.js",
                "lib/eventemitter2.chains.js",
                "lib/mediator.js",
                "routines/informers/document/ready.js",
                "routines/lib/jquery/jquery.js",
                "routines/ui/init/initbodyhider.js",
                "routines/ui/init/originalhider.js",
                "routines/ui/init/postinitbodyrevealer.js",
                "routines/ui/front/input/shower.js",
                "routines/ui/front/logo/shower.js",
                "routines/postboot.js"
            ],
            "matches": [
                "*://www.google.com/",
                "*://google.com/",
                "*://www.google.com/search*",
                "*://google.com/search*",
                "*://www.google.nl/",
                "*://google.nl/",
                "*://www.google.nl/search*",
                "*://google.nl/search*"
            ]
        }
    ]
}